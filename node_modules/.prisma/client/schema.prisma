generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Ballot {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  voterID      String @unique
  regPIN       String
  firstChoice  String
  secondChoice String
  thirdChoice  String
}
